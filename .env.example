# Inker Configuration
# Copy this file to .env and modify as needed:
#   cp .env.example .env

# Admin PIN for login (change this!)
ADMIN_PIN=1111

# Database password (change for production!)
POSTGRES_PASSWORD=inker_password

# Timezone for clock/date widgets
TZ=UTC

# Session signing key (change for production!)
JWT_SECRET=change-this-to-a-random-string-at-least-32-characters

# Frontend port (default: 80)
INKER_PORT=80
